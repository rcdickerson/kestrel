int c_1[N];
int a_2[N];
int b_2[N];
int c_2[N];

void main() {
  int l_k = arb_int();
  int l_x = arb_int();
  int r_k = arb_int();
  int r_x = arb_int();
  assume((l_k == r_k) && (l_x == r_x));
  int _i = 0;
  while (_i < N) {
    assume((a_1[_i] == a_2[_i]) && ((b_1[_i] == b_2[_i]) && (c_1[_i] == c_2[_i])));
  }
